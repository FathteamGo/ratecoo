{
  "name": "ratecoo",
  "version": "0.1.0",
  "private": true,
  "description": "Mini SaaS Review & Rating Widget",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "db:push": "pnpm --filter @ratecoo/db db:push",
    "db:studio": "pnpm --filter @ratecoo/db db:studio",
    "db:generate": "pnpm --filter @ratecoo/db db:generate"
  },
  "devDependencies": {
    "turbo": "^2.1.3",
    "typescript": "^5.6.3"
  }
}
